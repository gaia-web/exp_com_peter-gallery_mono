{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "articles",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "date": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "date",
      "required": true
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::countrie.countrie",
      "inversedBy": "articles"
    },
    "article_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::article-type.article-type"
    },
    "imageList": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "top": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
